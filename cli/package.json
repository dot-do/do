{
  "name": "@dotdo/cli",
  "version": "0.1.0",
  "description": "DO CLI - Publish, deploy, and manage Digital Objects",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./auth": {
      "types": "./src/auth.ts",
      "import": "./src/auth.ts"
    },
    "./config": {
      "types": "./src/config.ts",
      "import": "./src/config.ts"
    },
    "./parser": {
      "types": "./src/parser.ts",
      "import": "./src/parser.ts"
    },
    "./plugins": {
      "types": "./src/plugins.ts",
      "import": "./src/plugins.ts"
    },
    "./plugins/objects": {
      "types": "./src/plugins/objects.ts",
      "import": "./src/plugins/objects.ts"
    },
    "./objects": {
      "types": "./src/objects.ts",
      "import": "./src/objects.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "import": "./src/types.ts"
    }
  },
  "bin": {
    "dotdo": "./src/cli.ts",
    "do": "./src/cli.ts"
  },
  "files": [
    "src"
  ],
  "keywords": [
    "dotdo",
    "cli",
    "durable-objects",
    "cloudflare"
  ],
  "author": "Nathan Clevenger",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "rpc.do": "^0.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.0.0"
  }
}
