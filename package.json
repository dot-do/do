{
  "name": "@dotdo/do",
  "version": "0.1.6",
  "description": "Digital Object - Every business entity IS a Durable Object. Unified edge-native platform with CRUD APIs, SDK, AI, payments, and more.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./sdk": {
      "types": "./dist/sdk/index.d.ts",
      "import": "./dist/sdk/index.js"
    },
    "./sdk/rpc": {
      "types": "./dist/sdk/rpc.d.ts",
      "import": "./dist/sdk/rpc.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js"
    },
    "./api/routes": {
      "types": "./dist/api/routes/index.d.ts",
      "import": "./dist/api/routes/index.js"
    },
    "./api/middleware": {
      "types": "./dist/api/middleware/index.d.ts",
      "import": "./dist/api/middleware/index.js"
    },
    "./api/mcp": {
      "types": "./dist/api/mcp/index.d.ts",
      "import": "./dist/api/mcp/index.js"
    },
    "./db": {
      "types": "./dist/db/index.d.ts",
      "import": "./dist/db/index.js"
    },
    "./db/collections": {
      "types": "./dist/db/collections/index.d.ts",
      "import": "./dist/db/collections/index.js"
    },
    "./db/cdc": {
      "types": "./dist/db/cdc/index.d.ts",
      "import": "./dist/db/cdc/index.js"
    },
    "./db/storage": {
      "types": "./dist/db/storage/index.d.ts",
      "import": "./dist/db/storage/index.js"
    },
    "./rpc": {
      "types": "./dist/rpc/index.d.ts",
      "import": "./dist/rpc/index.js"
    },
    "./do": {
      "types": "./dist/do/index.d.ts",
      "import": "./dist/do/index.js"
    },
    "./ai": {
      "types": "./dist/ai/index.d.ts",
      "import": "./dist/ai/index.js"
    },
    "./oauth": {
      "types": "./dist/do/oauth.d.ts",
      "import": "./dist/do/oauth.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:workers": "vitest run --config vitest.workers.config.ts",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "dev": "wrangler dev --config proxy/wrangler.jsonc",
    "deploy": "wrangler deploy --config proxy/wrangler.jsonc",
    "deploy:dry-run": "wrangler deploy --config proxy/wrangler.jsonc --dry-run",
    "release": "pnpm build && changeset publish",
    "version": "changeset version"
  },
  "keywords": [
    "durable-objects",
    "cloudflare",
    "cloudflare-workers",
    "edge",
    "typescript",
    "rpc",
    "api",
    "crud",
    "sdk",
    "hono",
    "ai",
    "mcp",
    "digital-object"
  ],
  "author": "Nathan Clevenger",
  "license": "MIT",
  "homepage": "https://do.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dot-do/do.git"
  },
  "bugs": {
    "url": "https://github.com/dot-do/do/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "dependencies": {
    "@dotdo/apis": "^0.1.0",
    "@dotdo/oauth": "^0.1.1",
    "@dotdo/types": "^0.1.0",
    "commander": "^12.1.0",
    "ai-database": "^2.1.3",
    "ai-evaluate": "^2.1.5",
    "ai-functions": "^2.1.3",
    "ai-primitives": "^0.1.0",
    "ai-providers": "^2.1.3",
    "ai-workflows": "^2.1.3",
    "digital-tools": "^2.1.3",
    "digital-workers": "^2.1.3",
    "hono": "^4.7.0",
    "language-models": "^2.1.3",
    "mdxui": "^2.1.1",
    "oauth.do": "^0.2.0",
    "rpc.do": "^0.2.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@cloudflare/vitest-pool-workers": "^0.12.6",
    "@cloudflare/workers-types": "^4.20241230.0",
    "@vitest/coverage-v8": "^3.2.4",
    "jose": "^6.1.3",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0",
    "wrangler": "^4.60.0"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "@dotdo/types": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "@dotdo/types": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.15.4",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "tabWidth": 2,
    "printWidth": 160
  }
}
